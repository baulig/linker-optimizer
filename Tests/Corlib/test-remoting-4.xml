<?xml version="1.0" encoding="utf-8" ?>
<linker-optimizer>
<!--
	<options size-check-configuration="remoting" detailed-size-report="true" compare-size-with="remoting" check-size="test" profile="test" />
	-->

	<options all-modules="true" analyze-all="true" />

	<include filename="corlib-api.xml" />
	<include filename="corlib-asserts.xml" />
	<include filename="test-remoting-4-size.xml" />

	<type fullname="System.RuntimeType">
		<method name="ActivationCreateInstance" action="warn" />
	</type>

	<features>
		<feature name="remoting" enabled="true" />
		<feature name="collator" enabled="false" />
		<feature name="globalization" enabled="false" />
	</features>
</linker-optimizer>

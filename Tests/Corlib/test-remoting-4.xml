	<include filename="corlib-api.xml" />
	<include filename="corlib-asserts.xml" />

	<features>
		<feature name="remoting" enabled="true" />
		<feature name="collator" enabled="false" />
		<feature name="globalization" enabled="false" />
	</features>
</linker-optimizer>

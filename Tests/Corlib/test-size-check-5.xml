<?xml version="1.0" encoding="utf-8" ?>
<linker-optimizer>
	<options all-modules="true" preprocessor="full" report-size="true" analyze-all="true" />

	<conditional feature="security" enabled="false">
		<type fullname="System.AppDomain">
			<method name="get_DefaultPrincipal" action="throw" />
		</type>
	</conditional>

	<features>
		<feature name="security" enabled="false" />
		<feature name="remoting" enabled="false" />
		<feature name="serialization" enabled="false" />
		<feature name="encoding" enabled="false" />
		<feature name="collator" enabled="false" />
		<feature name="globalization" enabled="false" />
		<feature name="collator" enabled="false" />
		<feature name="crypto" enabled="false" />
	</features>
</linker-optimizer>

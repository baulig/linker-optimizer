<?xml version="1.0" encoding="utf-8" ?>
<linker>
	<martin>
		<options all-modules="true" analyze-all="true" preprocess="true" report-size="true" />

		<debug>
			<type name="HebrewNumber" action="fail" />

			<break-on method="System.DateTimeParse::TryParse" />
			<break-on method="DateTimeParse::MatchHebrewDigits" />
			<break-on method="DateTimeFormatInfo::TryParseHebrewNumber" />
		</debug>

		<features>
			<feature name="globalization" enabled="false" />
		</features>
	</martin>
</linker>

<?xml version="1.0" encoding="utf-8" ?>
<linker>
	<martin>
		<options all-modules="true" analyze-all="true" ignore-resolution-errors="true" preprocess="true" />

		<type fullname="System.Globalization.GlobalizationSupport">
			<method name="MartinTest" action="mark" />
		</type>

		<conditional feature="security" enabled="true">
			<!-- Internal interface, needed by System.Security.Permission tests -->
			<method substring="System.Security.Permissions.IBuiltInPermission.GetTokenIndex" action="none" />
		</conditional>

		<features>
			<feature name="globalization" enabled="true" />
			<feature name="security" enabled="true" />
		</features>
	</martin>
</linker>

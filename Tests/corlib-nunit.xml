<?xml version="1.0" encoding="utf-8" ?>
<linker>
	<martin>
		<options all-modules="true" analyze-all="true" ignore-resolution-errors="true" preprocess="true" main-debug="false" />

		<type fullname="System.Globalization.GlobalizationSupport" action="preserve" />

		<conditional feature="crypto" enabled="true">
			<type fullname="System.Security.Cryptography.CryptoConfig" action="preserve" />
		</conditional>

		<features>
			<feature name="globalization" enabled="true" />
			<feature name="security" enabled="true" />
		</features>
	</martin>
</linker>

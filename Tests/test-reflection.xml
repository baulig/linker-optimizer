<?xml version="1.0" encoding="utf-8" ?>
<linker>
	<martin>
		<options all-modules="true" analyze-all="true" preprocess="true" />

		<debug>
			<fail-on type="HebrewNumber" />
			<fail-on type="System.Reflection.Emit" />

			<break-on method="System.DateTimeParse::TryParse" />
			<break-on method="DateTimeParse::MatchHebrewDigits" />
			<break-on method="DateTimeFormatInfo::TryParseHebrewNumber" />
		</debug>

		<features>
			<feature name="globalization" enabled="false" />
			<feature name="sre" enabled="false" />
		</features>

		<preprocess>
			<type fullname="System.Globalization.ChineseLunisolarCalendar" mark="true" />
			<type fullname="System.Globalization.Calendar" mark="true" />
		</preprocess>
	</martin>
</linker>

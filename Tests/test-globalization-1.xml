<?xml version="1.0" encoding="utf-8" ?>
<linker>
	<martin>
		<options all-modules="true" analyze-all="true" preprocess="true" />

		<type name="HebrewNumber" action="fail" />

		<method substring="System.DateTimeParse::TryParse" action="debug"/>
		<method substring="DateTimeParse::MatchHebrewDigits" action="debug"/>
		<method substring="DateTimeFormatInfo::TryParseHebrewNumber" action="debug"/>
		
		<type fullname="Mono.Globalization.Unicode.SimpleCollator" action="fail" />

		<namespace name="System.Globalization">
			<type name="CompareInfo">
				<method name="get_UseManagedCollation" action="constant" />
			</type>
		</namespace>

		<features>
			<feature name="globalization" enabled="false" />
			<feature name="collator" enabled="false" />
		</features>
	</martin>
</linker>

<?xml version="1.0" encoding="utf-8" ?>
<linker>
	<martin>
		<options all-modules="true" analyze-all="true" preprocess="true" report-size="true" />

		<debug>
			<type name="SynchronizationAttribute" action="fail" />
			<!-- this one has a preserve entry -->
			<!-- <type fullname="System.Runtime.Remoting.RemotingServices" action="fail" /> -->
			<type fullname="System.Runtime.Serialization.FormatterServices" action="fail" />
			<type fullname="System.Runtime.Serialization.Formatters.Binary.ObjectReader" action="fail" />
			<method substring="System.Runtime.Serialization.FormatterServices::GetSafeUninitializedObject" action="fail" />
			<method substring="System.Runtime.Serialization.Formatters.Binary.BinaryFormatter::Deserialize" action="fail" />
		</debug>

		<features>
			<feature name="remoting" enabled="false" />
		</features>
	</martin>
</linker>

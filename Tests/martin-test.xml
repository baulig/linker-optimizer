<?xml version="1.0" encoding="utf-8" ?>
<linker>
	<martin>
		<options all-modules="false" analyze-all="true" preprocess="true" />

		<debug>
			<!--
			<fail-on type="HebrewNumber" />
			-->

			<break-on method="System.Void System.__ComObject::Finalize()" />
			<break-on method="SerializeExceptionData" />

			<!--
			<break-on method="DateTimeFormat::FormatCustomized" />
			<break-on method="GetCollator" />
			-->
		</debug>

		<features>
			<feature name="globalization" enabled="false" />
		</features>
	</martin>
</linker>

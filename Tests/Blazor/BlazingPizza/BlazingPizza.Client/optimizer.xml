<?xml version="1.0" encoding="utf-8" ?>
<linker-optimizer>

	<options report-configuration="blazor" disable-all="true" report-mode="actions+size+detailed" check-size="false" size-check-tolerance="10%" />

	<include filename="../../../Corlib/corlib-api.xml" />

	<namespace name="System.Linq.Expressions" action="none" />

	<namespace name="BlazingPizza.Client" action="preserve" />

	<namespace name="Microsoft.Extensions.DependencyInjection" action="none" />

	<type fullname="Microsoft.Extensions.DependencyInjection.ServiceProvider" action="none" />
	<type fullname="Microsoft.Extensions.DependencyInjection.ServiceLookup.ExpressionsServiceProviderEngine" action="fail" />
	<type fullname="Microsoft.Extensions.DependencyInjection.ServiceLookup.DynamicServiceProviderEngine" action="fail" />

	<features>
		<feature name="sre" enabled="false" />
		<feature name="remoting" enabled="false" />
		<feature name="collator" enabled="false" />
		<feature name="globalization" enabled="false" />
	</features>
</linker-optimizer>
